{
  "name": "webkid-react-starterkit",
  "version": "0.3.1",
  "description": "Lightweight starting point for a react web app.",
  "keywords": [
    "react",
    "starterkit",
    "frontend",
    "boilerplate",
    "reflux",
    "react-router"
  ],
  "main": "index.html",
  "scripts": {
    "start": "gulp",
    "test": "jest"
  },
  "author": "@moklick <mo@webkid.io>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^4.7.0",
    "babel-jest": "^4.0.0",
    "babel-loader": "^4.2.0",
    "babelify": "^6.1.1",
    "browserify": "^10.2.0",
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-babel": "^5.1.0",
    "gulp-concat": "^2.5.2",
    "gulp-connect": "^2.2.0",
    "gulp-livereload": "^3.8.0",
    "gulp-load-plugins": "^0.9.0",
    "gulp-minify-css": "^1.1.1",
    "gulp-minify-html": "^1.0.2",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^1.3.3",
    "gulp-size": "^1.2.1",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-uglifyjs": "^0.6.1",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.2.1",
    "gulp-webpack": "^1.3.0",
    "jest-cli": "^0.4.0",
    "merge-stream": "^0.1.7",
    "object-assign": "^2.0.0",
    "react-tools": "^0.13.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "react": "^0.13.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/object-assign",
      "<rootDir>/test-utils/stubRouterContext.jsx"
    ]
  }
}
